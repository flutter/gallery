{
  "hosting": [
    {
      "target": "staging",
      "public": "build/web",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "prod",
      "public": "build/web",
      "redirects": [
        {
          "source": "/",
          "destination": "https://docs.flutter.dev/gallery",
          "type": 301
        }
      ],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ]
}
